#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules/import-local/fixtures/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules/import-local/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/fixtures/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.1.0/node_modules/import-local/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/import-local@3.1.0/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules/import-local/fixtures/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules/import-local/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../import-local/fixtures/cli.js" "$@"
else
  exec node  "$basedir/../import-local/fixtures/cli.js" "$@"
fi
