#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-webpack-plugin@4.5.0_webpack@4.44.2/node_modules/html-webpack-plugin/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-webpack-plugin@4.5.0_webpack@4.44.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules/html-minifier-terser/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-minifier-terser@5.1.1/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-webpack-plugin@4.5.0_webpack@4.44.2/node_modules/html-webpack-plugin/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/html-webpack-plugin@4.5.0_webpack@4.44.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
else
  exec node  "$basedir/../../../../../html-minifier-terser@5.1.1/node_modules/html-minifier-terser/cli.js" "$@"
fi
