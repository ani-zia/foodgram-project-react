#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/bin/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/bin/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules/webpack-dev-server/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/webpack-dev-server@3.11.0_webpack@4.44.2/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../bin/webpack-dev-server.js" "$@"
fi
