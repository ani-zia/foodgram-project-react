#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/escodegen@2.0.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/bin/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/esprima@4.0.1/node_modules/esprima/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/esprima@4.0.1/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules:/Users/annasimanova/Dev/foodgram-project-react/node_modules:/Users/annasimanova/Dev/node_modules:/Users/annasimanova/node_modules:/Users/node_modules:/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/escodegen@2.0.0/node_modules/escodegen/node_modules:/Users/annasimanova/Dev/foodgram-project-react/frontend/node_modules/.pnpm/escodegen@2.0.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../../../../../esprima@4.0.1/node_modules/esprima/bin/esvalidate.js" "$@"
fi
